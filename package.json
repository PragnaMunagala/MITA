{
  "name": "mita",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "node server/server.js",
    "start:dev": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --code-coverage",
    "test:dev": "ng test --watch --code-coverage",
    "test:ubuntu": "ng test --watch=false --code-coverage --browsers ChromeHeadlessNoSandbox",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "version:up": "ts-node versioning/git.version.ts",
    "version:tag": "ts-node versioning/git.version-tag.ts",
    "build": "NODE_ENV='production' ng build --prod --aot --base-href /MITA/ && tar cJf deploy/MITA.tar.xz -C dist MITA",
    "build:staging": "NODE_ENV='development' ng build --environment=dev --aot --base-href /MITA/ && tar cJf deploy/MITA.tar.xz -C dist MITA",
    "deploy": "deploy/deploy.sh --prod && rimraf dist",
    "deploy:staging": "deploy/deploy.sh --dev && rimraf dist"
  },
  "private": true,
  "dependencies": {
    "classlist.js": "^1.1.20150312",
    "express": "^4.14.0",
    "intl": "^1.2.5",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@angular/cli": "1.4.4",
    "@angular/common": "4.3.2",
    "@angular/compiler": "4.3.2",
    "@angular/compiler-cli": "4.3.2",
    "@angular/core": "4.3.2",
    "@angular/forms": "4.3.2",
    "@angular/http": "4.3.2",
    "@angular/platform-browser": "4.3.2",
    "@angular/platform-browser-dynamic": "4.3.2",
    "@angular/router": "4.3.2",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.26",
    "@types/auth0-lock": "^10.10.1",
    "@types/jasmine": "2.5.47",
    "@types/lodash": "^4.14.44",
    "@types/node": "^7.0.18",
    "angular2-jwt": "^0.2.3",
    "auth0-lock": "^10.16.0",
    "codelyzer": "~3.0.1",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "lodash": "^4.17.2",
    "protractor": "~5.1.0",
    "rxjs": "^5.5.6",
    "ts-helpers": "^1.1.1",
    "ts-node": "^3.0.6",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "zone.js": "^0.8.12"
  }
}
